---
import fetchApi from "../lib/strapi";
import type { metaTypes } from "../types/meta-types";

const meta = await fetchApi<metaTypes>({
  endpoint: "meta", // the content type to fetch
});
---

<header class="condor">
  <h1 class="serif">
    <a href="/">
      {meta.name}<br />
      {meta.title}
    </a>
  </h1>
</header>

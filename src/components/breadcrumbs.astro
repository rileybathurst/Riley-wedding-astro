---
const { items } = Astro.props;
type BreadcrumbTypes = {
  label: string;
  href?: string /* the last doesnt have a link */;
};
---

<ol class="breadcrumbs condor">
  {
    items.map((item: BreadcrumbTypes) => (
      <li>
        <a href={item.href}>{item.label}</a>
      </li>
    ))
  }
</ol>
